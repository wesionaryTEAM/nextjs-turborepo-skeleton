{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    },
    "cypress:open": {
      "cache": false
    },
    "cypress:run": {
      "cache": false
    },
    "e2e:open": {
      "cache": false
    },
    "deploy": {
      "dependsOn": ["lint"],
      "outputs": []
    }
  }
}
